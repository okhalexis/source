-- NO CLIP
----------------------------------------------------
local NoClipSection = Tabs.Player:Section({
    Title  = "No Clip",
    Icon   = "ghost",
    Box    = true,
    Opened = true,
})

local function applyFullNoClip()
    local char = getCharacter()
    if not char then return end
    for _, part in ipairs(char:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = false
        end
    end
end

local function disableFullNoClip()
    local char = getCharacter()
    if not char then return end
    for _, part in ipairs(char:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = true
        end
    end
end

RunService.Stepped:Connect(function()
    if PlayerFlags.NoClipEnabled then
        applyFullNoClip()
    end
end)

NoClipSection:Toggle({
    Title = "No Clip",
    Desc  = "Walk through any wall.",
    Icon  = "ghost",
    Type  = "Checkbox",
    Value = false,
    Callback = function(state)
        PlayerFlags.NoClipEnabled = state
        if not state then
            disableFullNoClip()
        end
    end
})

----------------------------------------------------
-- FARM / INSTANT STEAL
----------------------------------------------------
local FarmSection = Tabs.Farm:Section({
    Title  = "Instant Steal",
    Icon   = "zap",
    Box    = true,
    Opened = true,
})

local connections = {}

local function setPromptInstant(prompt)
    prompt.HoldDuration = 0
end

local function enableInstantSteal()
    for _, prompt in ipairs(workspace:GetDescendants()) do
        if prompt:IsA("ProximityPrompt") then
            setPromptInstant(prompt)
        end
    end

    table.insert(connections,
        ProximityPromptService.PromptShown:Connect(function(prompt)
            setPromptInstant(prompt)
        end)
    )

    table.insert(connections,
        ProximityPromptService.PromptButtonHoldBegan:Connect(function(prompt, player)
            if not PlayerFlags.InstantSteal then return end
            if player ~= LocalPlayer then return end
            setPromptInstant(prompt)
            pcall(function()
                fireproximityprompt(prompt)
            end)
        end)
    )
end

local function disableInstantSteal()
    for _, conn in ipairs(connections) do
        if conn.Connected then
            conn:Disconnect()
        end
    end
    table.clear(connections)
end

FarmSection:Toggle({
    Title = "Instant Steal",
    Desc  = "Quick Interact with all prompts!",
    Icon  = "zap",
    Value = false,
    Callback = function(state)
        PlayerFlags.InstantSteal = state
        if state then
            disableInstantSteal()
            enableInstantSteal()
            WindUI:Notify({
                Title   = "Instant Steal",
                Content = "Instant Steal enabled (Quick Interact).",
                Duration= 3,
                Icon    = "zap",
            })
        else
            disableInstantSteal()
            WindUI:Notify({
                Title   = "Instant Steal",
                Content = "Instant Steal disabled.",
                Duration= 3,
                Icon    = "zap",
            })
        end
    end
})

----------------------------------------------------
-- AUTO FARM BRAINROT - L√ìGICA FINAL CORRETA
----------------------------------------------------
local AutoFarmSection = Tabs.Farm:Section({
    Title  = "Auto Farm Brainrot",
    Icon   = "cpu",
    Box    = true,
    Opened = true,
})

-- POSI√á√ïES FIXAS COM FORMATO X, Y, Z
local GapPositions = {
    { Index = 1,  Name = "Start Position",  X = 150.03,  Y = 3.26,    Z = -9.30 },
    { Index = 2,  Name = "Common",          X = 201.99,  Y = -2.74,   Z = -9.46 },
    { Index = 3,  Name = "Uncommon",        X = 288.75,  Y = -2.74,   Z = -33.45 },
    { Index = 4,  Name = "Rare",            X = 399.06,  Y = -2.74,   Z = -39.60 },
    { Index = 5,  Name = "Epic",            X = 544.87,  Y = -2.74,   Z = -34.12 },
    { Index = 6,  Name = "Legendary",       X = 761.22,  Y = -2.74,   Z = -37.66 },
    { Index = 7,  Name = "Mythical",        X = 1081.41, Y = -2.74,   Z = -6.72 },
    { Index = 8,  Name = "Cosmic",          X = 1552.26, Y = -2.74,   Z = -17.41 },
    { Index = 9,  Name = "Secret",          X = 2272.14, Y = -2.74,   Z = -47.31 },
    { Index = 10, Name = "Celestial",       X = 2591.95, Y = -2.74,   Z = 12.74 },
}

local AutoGapState = {
    Enabled = false,
    TargetGapIndex = nil,
    CurrentPositionIndex = 0,
    HasTeleported = false,
}

--[[========================================================
    SISTEMA DE DETEC√á√ÉO DE WAVES
==========================================================]]

local WaveDetector = {}

local wavesCache = {}
local lastWaveScan = 0
local WAVE_SCAN_INTERVAL = 0.5

function WaveDetector.ScanAllWaves()
    local currentTime = tick()
    
    if currentTime - lastWaveScan < WAVE_SCAN_INTERVAL then
        return wavesCache
    end
    
    lastWaveScan = currentTime
    wavesCache = {}
    
    for _, obj in ipairs(workspace:GetChildren()) do
        local name = obj.Name:lower()
        
        if name:find("tsunami") or 
           name:find("wave") or 
           name:find("water") or 
           name:find("snake") or 
           name:find("wacky") or
           name:find("flood") then
            
            local part = nil
            if obj:IsA("Model") then
                part = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
            elseif obj:IsA("BasePart") then
                part = obj
            end
            
            if part then
                table.insert(wavesCache, {
                    Name = obj.Name,
                    Position = part.Position,
                })
            end
        end
    end
    
    return wavesCache
end

function WaveDetector.GetWaveGapPosition(wavePos)
    local closestGapIndex = nil
    local shortestDist = math.huge
    
    for i, gap in ipairs(GapPositions) do
        local gapPos = Vector3.new(gap.X, gap.Y, gap.Z)
        local dist = (wavePos - gapPos).Magnitude
        
        if dist < shortestDist then
            shortestDist = dist
            closestGapIndex = i
        end
    end
    
    return closestGapIndex
end

-- L√ìGICA CORRETA:
-- Pode mover se wave est√° 2+ gaps √† frente OU j√° passou (atr√°s)
-- Espera se wave est√° no mesmo gap OU apenas 1 gap √† frente
function WaveDetector.CanMoveToNextGap(currentGapIndex)
    local waves = WaveDetector.ScanAllWaves()
    
    if #waves == 0 then
        return true
    end
    
    for _, wave in ipairs(waves) do
        local waveGapIndex = WaveDetector.GetWaveGapPosition(wave.Position)
        
        if waveGapIndex then
            local difference = waveGapIndex - currentGapIndex
            
            -- Wave atr√°s (j√° passou) ‚Üí IGNORA, n√£o importa
            if difference < 0 then
                -- N√£o faz nada, continua checando outras waves
                
            -- Wave no mesmo gap ‚Üí ESPERA
            elseif difference == 0 then
                print(string.format("[Wave Detector] ‚ö†Ô∏è Wave at Gap %d (SAME as player) - WAITING", waveGapIndex))
                return false
                
            -- Wave 1 gap √† frente ‚Üí ESPERA
            elseif difference == 1 then
                print(string.format("[Wave Detector] ‚ö†Ô∏è Wave at Gap %d (1 ahead) - WAITING", waveGapIndex))
                return false
                
            -- Wave 2+ gaps √† frente ‚Üí PODE MOVER (wave est√° longe)
            else
                print(string.format("[Wave Detector] ‚úÖ Wave at Gap %d (%d ahead) - SAFE", waveGapIndex, difference))
                -- Continua checando outras waves
            end
        end
    end
    
    return true
end

--[[========================================================
    SISTEMA DE MOVIMENTO
==========================================================]]

local function instantTeleport(gap)
    local hrp = getHumanoidRootPart()
    if not hrp then return false end
    
    local pos = Vector3.new(gap.X, gap.Y + 2, gap.Z)
    hrp.CFrame = CFrame.new(pos)
    return true
end

--[[========================================================
    MAIN LOOP
==========================================================]]

local autoFarmCoroutine = nil

local function autoFarmLoop()
    while AutoGapState.Enabled do
        if not AutoGapState.TargetGapIndex then
            task.wait(0.5)
            continue
        end
        
        local hrp = getHumanoidRootPart()
        if not hrp then
            task.wait(0.5)
            continue
        end
        
        -- Chegou no destino final
        if AutoGapState.CurrentPositionIndex >= AutoGapState.TargetGapIndex then
            WindUI:Notify({
                Title   = "Auto Farm",
                Content = "‚úÖ Arrived at " .. GapPositions[AutoGapState.TargetGapIndex].Name,
                Duration= 3,
                Icon    = "check-circle",
            })
            AutoGapState.Enabled = false
            break
        end
        
        -- Calcula pr√≥xima posi√ß√£o
        local nextPositionIndex = AutoGapState.CurrentPositionIndex + 1
        local nextGap = GapPositions[nextPositionIndex]
        
        if not nextGap then
            task.wait(0.5)
            continue
        end
        
        -- Verifica se j√° est√° na posi√ß√£o
        local currentPos = hrp.Position
        local nextPos = Vector3.new(nextGap.X, nextGap.Y, nextGap.Z)
        local distToNext = (currentPos - nextPos).Magnitude
        
        if distToNext < 15 then
            -- J√° chegou
            AutoGapState.CurrentPositionIndex = nextPositionIndex
            AutoGapState.HasTeleported = false
            print(string.format("[Auto Farm] ‚úÖ Reached %s (Gap %d)", nextGap.Name, nextPositionIndex))
            
            if AutoGapState.CurrentPositionIndex >= AutoGapState.TargetGapIndex then
                WindUI:Notify({
                    Title   = "Auto Farm",
                    Content = "‚úÖ Arrived at " .. nextGap.Name,
                    Duration= 3,
                    Icon    = "check-circle",
                })
                AutoGapState.Enabled = false
                break
            end
            
            task.wait(0.5)
            continue
        end
        
        -- Verifica se pode mover
        local canMove = WaveDetector.CanMoveToNextGap(AutoGapState.CurrentPositionIndex)
        
        if canMove then
            if not AutoGapState.HasTeleported then
                print(string.format("[Auto Farm] üöÄ Teleporting to %s (Gap %d)", nextGap.Name, nextPositionIndex))
                
                local success = instantTeleport(nextGap)
                
                if success then
                    AutoGapState.HasTeleported = true
                    task.wait(0.3)
                end
            end
        else
            -- Espera
            AutoGapState.HasTeleported = false
            
            local currentGapName = GapPositions[math.max(1, AutoGapState.CurrentPositionIndex)].Name
            print(string.format("[Auto Farm] ‚è∏Ô∏è Waiting at %s...", currentGapName))
            
            WindUI:Notify({
                Title   = "Auto Farm",
                Content = "‚è∏Ô∏è Waiting for wave...",
                Duration= 2,
                Icon    = "pause-circle",
            })
            
            task.wait(1)
        end
        
        task.wait(0.1)
    end
end

--[[========================================================
    UI CONTROLS
==========================================================]]

local function getAreaNames()
    local names = {}
    for i = 2, #GapPositions do
        table.insert(names, GapPositions[i].Name)
    end
    return names
end

AutoFarmSection:Dropdown({
    Title  = "Select Area",
    Desc   = "Choose target area",
    Icon   = "map-pin",
    Values = getAreaNames(),
    Value  = nil,
    Callback = function(value)
        for i, gap in ipairs(GapPositions) do
            if gap.Name == value then
                AutoGapState.TargetGapIndex = i
                
                WindUI:Notify({
                    Title   = "Auto Farm",
                    Content = "Selected: " .. value,
                    Duration= 3,
                    Icon    = "map-pin",
                })
                break
            end
        end
    end
})

AutoFarmSection:Toggle({
    Title = "Go to Area",
    Desc  = "Auto navigate to selected area",
    Icon  = "navigation",
    Value = false,
    Callback = function(state)
        AutoGapState.Enabled = state
        
        if state then
            if not AutoGapState.TargetGapIndex then
                WindUI:Notify({
                    Title   = "Auto Farm",
                    Content = "‚ö†Ô∏è Select an area first!",
                    Duration= 3,
                    Icon    = "alert-triangle",
                })
                AutoGapState.Enabled = false
                return
            end
            
            AutoGapState.CurrentPositionIndex = 0
            AutoGapState.HasTeleported = false
            
            WindUI:Notify({
                Title   = "Auto Farm",
                Content = "üöÄ Starting navigation to " .. GapPositions[AutoGapState.TargetGapIndex].Name,
                Duration= 3,
                Icon    = "navigation",
            })
            
            if autoFarmCoroutine then
                task.cancel(autoFarmCoroutine)
            end
            autoFarmCoroutine = task.spawn(autoFarmLoop)
        else
            if autoFarmCoroutine then
                task.cancel(autoFarmCoroutine)
                autoFarmCoroutine = nil
            end
            
            WindUI:Notify({
                Title   = "Auto Farm",
                Content = "Stopped.",
                Duration= 3,
                Icon    = "square",
            })
        end
    end
})

