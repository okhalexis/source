-- Movement system
local AutoGapVel, AutoGapGyro

local function ensureMovers()
    local hrp = getHumanoidRootPart()
    if not hrp then return end
    
    if AutoGapVel and AutoGapVel.Parent ~= hrp then
        AutoGapVel:Destroy()
        AutoGapVel = nil
    end
    
    if AutoGapGyro and AutoGapGyro.Parent ~= hrp then
        AutoGapGyro:Destroy()
        AutoGapGyro = nil
    end
    
    if not AutoGapVel then
        AutoGapVel = Instance.new("BodyVelocity")
        AutoGapVel.MaxForce = Vector3.new(1e5, 1e5, 1e5)
        AutoGapVel.Velocity = Vector3.zero
        AutoGapVel.Parent = hrp
    end
    
    if not AutoGapGyro then
        AutoGapGyro = Instance.new("BodyGyro")
        AutoGapGyro.MaxTorque = Vector3.new(1e5, 1e5, 1e5)
        AutoGapGyro.P = 9e4
        AutoGapGyro.CFrame = hrp.CFrame
        AutoGapGyro.Parent = hrp
    end
    
    return AutoGapVel, AutoGapGyro, hrp
end

local function stopMovement()
    if AutoGapVel then
        AutoGapVel.Velocity = Vector3.zero
    end
end

-- Main loop
RunService.Heartbeat:Connect(function(dt)
    if not AutoGapState.Enabled then
        stopMovement()
        GapScanner.ClearVisualizer()
        return
    end

    if not AutoGapState.SelectedArea then
        stopMovement()
        return
    end

    local area = getAreaByName(AutoGapState.SelectedArea)
    if not area or #area.Gaps == 0 then
        stopMovement()
        return
    end

    local bodyVel, bodyGyro, hrp = ensureMovers()
    if not bodyVel or not bodyGyro or not hrp then
        return
    end

    local gaps = area.Gaps
    
    if AutoGapState.ShowVisualizer and #VisualizerParts == 0 then
        GapScanner.CreateVisualizer(gaps)
    elseif not AutoGapState.ShowVisualizer and #VisualizerParts > 0 then
        GapScanner.ClearVisualizer()
    end
    
    local idx = math.clamp(AutoGapState.CurrentGapIdx, 1, #gaps)
    local currentGapCF = gaps[idx]
    
    if typeof(currentGapCF) ~= "CFrame" then
        stopMovement()
        return
    end

    local hrpPos = hrp.Position
    local targetPos = currentGapCF.Position
    local distToGap = (targetPos - hrpPos).Magnitude
    local insideGap = distToGap < 8

    local wavePos, waveVel, waveSize, waveMethod = WaveDetector.GetWaveInfo()

    if not insideGap then
        local dir = (targetPos - hrpPos).Unit
        bodyVel.Velocity = dir * AutoGapState.MovementSpeed
        bodyGyro.CFrame = CFrame.lookAt(hrpPos, targetPos)
        return
    end

    local nextIdx = idx + 1
    if nextIdx > #gaps then
        stopMovement()
        WindUI:Notify({
            Title   = "Auto Gap",
            Content = "Reached final gap!",
            Duration= 3,
            Icon    = "check-circle",
        })
        AutoGapState.Enabled = false
        return
    end

    local nextGapCF = gaps[nextIdx]
    if typeof(nextGapCF) ~= "CFrame" then
        stopMovement()
        return
    end

    local nextPos = nextGapCF.Position
    local timeToNext = estimateTravelTime(hrpPos, nextPos)
    local timeWaveToNext = estimateCollisionTime(wavePos, waveVel, nextPos)

    local safeSpeed = math.max(math.abs((waveVel and waveVel.Z) or 0), 1)
    local safeMargin = AutoGapState.SafeDistance / safeSpeed

    local canMove = (timeToNext + safeMargin) < timeWaveToNext

    if AutoGapState.WaitMode and not canMove then
        stopMovement()
        return
    end

    if canMove then
        AutoGapState.CurrentGapIdx = nextIdx
        local dir = (nextPos - hrpPos).Unit
        bodyVel.Velocity = dir * AutoGapState.MovementSpeed
        bodyGyro.CFrame = CFrame.lookAt(hrpPos, nextPos)
    else
        stopMovement()
    end
end)

-- Auto-scan ao iniciar
task.spawn(function()
    task.wait(3)
    AutoPopulateGaps()
end)

