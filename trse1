-- Database de gaps por categoria
local GapDatabase = {
    { Key = "Common",    Name = "Common",    Icon = "circle",   Gaps = {} },
    { Key = "Uncommon",  Name = "Uncommon",  Icon = "disc",     Gaps = {} },
    { Key = "Rare",      Name = "Rare",      Icon = "star",     Gaps = {} },
    { Key = "Epic",      Name = "Epic",      Icon = "zap",      Gaps = {} },
    { Key = "Legendary", Name = "Legendary", Icon = "award",    Gaps = {} },
    { Key = "Mythical",  Name = "Mythical",  Icon = "flame",    Gaps = {} },
    { Key = "Cosmic",    Name = "Cosmic",    Icon = "moon",     Gaps = {} },
    { Key = "Secret",    Name = "Secret",    Icon = "lock",     Gaps = {} },
    { Key = "Celestial", Name = "Celestial", Icon = "sparkles", Gaps = {} },
}

local AutoGapState = {
    Enabled        = false,
    SelectedArea   = nil,
    SafeDistance   = 120,
    CurrentGapIdx  = 1,
    MovementSpeed  = 50,
    ShowVisualizer = false,
    WaitMode       = false,
}

-- Sistema de detec√ß√£o de wave (multi-m√©todo)
local WaveDetector = {}

function WaveDetector.FindByAttribute()
    if workspace:GetAttribute("SnakeWavesActive") == true then
        for _, obj in workspace:GetChildren() do
            if obj.Name:lower():find("snake") and (obj:IsA("Model") or obj:IsA("Part")) then
                return obj, "SnakeWave"
            end
        end
    end
    
    if workspace:GetAttribute("WackyWavesActive") == true then
        for _, obj in workspace:GetChildren() do
            if obj.Name:lower():find("wacky") and (obj:IsA("Model") or obj:IsA("Part")) then
                return obj, "WackyWave"
            end
        end
    end
    
    return nil, nil
end

function WaveDetector.FindByName()
    for _, obj in workspace:GetChildren() do
        local name = obj.Name:lower()
        if (name:find("tsunami") or name:find("wave") or name:find("water")) and (obj:IsA("Model") or obj:IsA("BasePart")) then
            return obj, "NameDetected"
        end
    end
    return nil, nil
end

function WaveDetector.FindByVelocity()
    for _, obj in workspace:GetDescendants() do
        if obj:IsA("BasePart") and not obj.Anchored then
            local vel = obj.AssemblyLinearVelocity
            if vel and vel.Magnitude > 15 then
                return obj, "VelocityDetected"
            end
        end
    end
    return nil, nil
end

function WaveDetector.FindByTag()
    for _, obj in CollectionService:GetTagged("Wave") do
        if obj:IsA("BasePart") or obj:IsA("Model") then
            return obj, "TagDetected"
        end
    end
    return nil, nil
end

function WaveDetector.GetWave()
    local wave, method = WaveDetector.FindByAttribute()
    if wave then return wave, method end
    
    wave, method = WaveDetector.FindByName()
    if wave then return wave, method end
    
    wave, method = WaveDetector.FindByTag()
    if wave then return wave, method end
    
    wave, method = WaveDetector.FindByVelocity()
    if wave then return wave, method end
    
    return nil, nil
end

function WaveDetector.GetPrimaryPart(wave)
    if not wave then return nil end
    if wave:IsA("BasePart") then return wave end
    if wave:IsA("Model") then
        return wave.PrimaryPart or wave:FindFirstChildWhichIsA("BasePart")
    end
    return nil
end

function WaveDetector.GetWaveInfo()
    local wave, method = WaveDetector.GetWave()
    local part = WaveDetector.GetPrimaryPart(wave)
    if not part then return nil, nil, nil, nil end
    
    return part.Position, part.AssemblyLinearVelocity or Vector3.zero, part.Size, method
end

-- Scanner autom√°tico de gaps
local GapScanner = {}

function GapScanner.ScanByHeight(minHeight, maxHeight, minSize)
    minHeight = minHeight or 50
    maxHeight = maxHeight or 500
    minSize = minSize or Vector3.new(6, 3, 6)
    
    local gaps = {}
    local processed = {}
    
    for _, obj in workspace:GetDescendants() do
        if obj:IsA("BasePart") and obj.Anchored then
            local pos = obj.Position
            local size = obj.Size
            
            if pos.Y >= minHeight and pos.Y <= maxHeight then
                if size.X >= minSize.X and size.Z >= minSize.Z then
                    local isDupe = false
                    for _, p in processed do
                        if (pos - p).Magnitude < 10 then
                            isDupe = true
                            break
                        end
                    end
                    
                    if not isDupe then
                        table.insert(gaps, {
                            Position = pos,
                            CFrame = CFrame.new(pos),
                            Part = obj,
                            Size = size,
                            Height = pos.Y,
                        })
                        table.insert(processed, pos)
                    end
                end
            end
        end
    end
    
    table.sort(gaps, function(a, b)
        return a.Position.Z < b.Position.Z
    end)
    
    return gaps
end

function GapScanner.ScanByTag(tag)
    tag = tag or "Gap"
    local gaps = {}
    
    for _, obj in CollectionService:GetTagged(tag) do
        if obj:IsA("BasePart") then
            table.insert(gaps, {
                Position = obj.Position,
                CFrame = obj.CFrame,
                Part = obj,
                Size = obj.Size,
                Height = obj.Position.Y,
            })
        end
    end
    
    table.sort(gaps, function(a, b)
        return a.Position.Z < b.Position.Z
    end)
    
    return gaps
end

function GapScanner.ScanByName(...)
    local names = {...}
    if #names == 0 then names = {"gap", "platform", "safe"} end
    
    local gaps = {}
    local processed = {}
    
    for _, obj in workspace:GetDescendants() do
        if obj:IsA("BasePart") and obj.Anchored then
            local name = obj.Name:lower()
            for _, searchName in names do
                if name:find(searchName:lower()) then
                    local pos = obj.Position
                    local isDupe = false
                    for _, p in processed do
                        if (pos - p).Magnitude < 10 then
                            isDupe = true
                            break
                        end
                    end
                    
                    if not isDupe then
                        table.insert(gaps, {
                            Position = pos,
                            CFrame = obj.CFrame,
                            Part = obj,
                            Size = obj.Size,
                            Height = pos.Y,
                        })
                        table.insert(processed, pos)
                    end
                    break
                end
            end
        end
    end
    
    table.sort(gaps, function(a, b)
        return a.Position.Z < b.Position.Z
    end)
    
    return gaps
end

function GapScanner.CategorizeByHeight(gaps)
    local categorized = {
        Common = {},
        Uncommon = {},
        Rare = {},
        Epic = {},
        Legendary = {},
        Mythical = {},
        Cosmic = {},
        Secret = {},
        Celestial = {},
    }
    
    for _, gap in gaps do
        local h = gap.Height
        
        if h < 100 then
            table.insert(categorized.Common, gap.CFrame)
        elseif h < 150 then
            table.insert(categorized.Uncommon, gap.CFrame)
        elseif h < 200 then
            table.insert(categorized.Rare, gap.CFrame)
        elseif h < 250 then
            table.insert(categorized.Epic, gap.CFrame)
        elseif h < 300 then
            table.insert(categorized.Legendary, gap.CFrame)
        elseif h < 350 then
            table.insert(categorized.Mythical, gap.CFrame)
        elseif h < 400 then
            table.insert(categorized.Cosmic, gap.CFrame)
        elseif h < 450 then
            table.insert(categorized.Secret, gap.CFrame)
        else
            table.insert(categorized.Celestial, gap.CFrame)
        end
    end
    
    return categorized
end

local VisualizerParts = {}

function GapScanner.CreateVisualizer(gaps)
    GapScanner.ClearVisualizer()
    
    if not AutoGapState.ShowVisualizer then return end
    
    for i, gap in gaps do
        local part = Instance.new("Part")
        part.Name = "GapVis_" .. i
        part.Size = Vector3.new(4, 1, 4)
        part.CFrame = gap.CFrame or CFrame.new(gap.Position)
        part.Anchored = true
        part.CanCollide = false
        part.Transparency = 0.5
        part.Color = Color3.fromRGB(0, 255, 0)
        part.Material = Enum.Material.Neon
        part.Parent = workspace.CurrentCamera
        
        local bill = Instance.new("BillboardGui")
        bill.Size = UDim2.new(0, 50, 0, 50)
        bill.Adornee = part
        bill.AlwaysOnTop = true
        bill.Parent = part
        
        local lbl = Instance.new("TextLabel")
        lbl.Size = UDim2.new(1, 0, 1, 0)
        lbl.BackgroundTransparency = 1
        lbl.Text = tostring(i)
        lbl.TextColor3 = Color3.new(1, 1, 1)
        lbl.TextScaled = true
        lbl.Font = Enum.Font.GothamBold
        lbl.Parent = bill
        
        table.insert(VisualizerParts, part)
    end
end

function GapScanner.ClearVisualizer()
    for _, part in VisualizerParts do
        if part and part.Parent then
            part:Destroy()
        end
    end
    table.clear(VisualizerParts)
end

-- Fun√ß√£o de populate autom√°tico
local function AutoPopulateGaps()
    print("[Auto Gap] üîç Scanning for gaps...")
    
    local taggedGaps = GapScanner.ScanByTag("Gap")
    
    if #taggedGaps > 0 then
        print("[Auto Gap] ‚úÖ Found", #taggedGaps, "tagged gaps")
        local categorized = GapScanner.CategorizeByHeight(taggedGaps)
        
        for i, area in ipairs(GapDatabase) do
            if categorized[area.Key] and #categorized[area.Key] > 0 then
                GapDatabase[i].Gaps = categorized[area.Key]
                print("[Auto Gap]", area.Key, "‚Üí", #categorized[area.Key], "gaps")
            end
        end
    else
        print("[Auto Gap] ‚ö†Ô∏è No tagged gaps, scanning by height...")
        local scannedGaps = GapScanner.ScanByHeight(50, 500, Vector3.new(6, 3, 6))
        
        if #scannedGaps > 0 then
            print("[Auto Gap] ‚úÖ Found", #scannedGaps, "potential gaps")
            local categorized = GapScanner.CategorizeByHeight(scannedGaps)
            
            for i, area in ipairs(GapDatabase) do
                if categorized[area.Key] and #categorized[area.Key] > 0 then
                    GapDatabase[i].Gaps = categorized[area.Key]
                    print("[Auto Gap]", area.Key, "‚Üí", #categorized[area.Key], "gaps")
                end
            end
        else
            warn("[Auto Gap] ‚ùå No gaps found! Trying name scan...")
            local namedGaps = GapScanner.ScanByName("gap", "platform", "safe", "stand")
            
            if #namedGaps > 0 then
                print("[Auto Gap] ‚úÖ Found", #namedGaps, "named gaps")
                local categorized = GapScanner.CategorizeByHeight(namedGaps)
                
                for i, area in ipairs(GapDatabase) do
                    if categorized[area.Key] and #categorized[area.Key] > 0 then
                        GapDatabase[i].Gaps = categorized[area.Key]
                        print("[Auto Gap]", area.Key, "‚Üí", #categorized[area.Key], "gaps")
                    end
                end
            else
                warn("[Auto Gap] ‚ùå No gaps detected by any method!")
            end
        end
    end
end

-- Helpers
local function getAreaNames()
    local names = {}
    for _, area in ipairs(GapDatabase) do
        table.insert(names, area.Name)
    end
    return names
end

local function getAreaByName(name)
    for _, area in ipairs(GapDatabase) do
        if area.Name == name then
            return area
        end
    end
    return nil
end

local function estimateCollisionTime(wavePos, waveVel, targetPos)
    if not wavePos or not waveVel or not targetPos then return math.huge end
    
    local dirZ = waveVel.Z
    if math.abs(dirZ) < 1 then return math.huge end
    
    local distZ = targetPos.Z - wavePos.Z
    if (dirZ > 0 and distZ < 0) or (dirZ < 0 and distZ > 0) then
        return math.huge
    end
    
    return math.abs(distZ) / math.abs(dirZ)
end

local function estimateTravelTime(fromPos, toPos)
    local dist = (toPos - fromPos).Magnitude
    local speed = math.max(AutoGapState.MovementSpeed, 1)
    return dist / speed
end

